{
    "_version": "1.12.0",
    "sap.app": {
        "id": "sap.ui.inventory",
        "type": "application",
        "title": "Inventory",
        "description": "Inventory Management Solution",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes":{
            "desktop": true,
		    "tablet": true,
		    "phone": true
        }   
    },
    "sap.ui5" :{
        "rootView":{
            "viewName": "sap.ui.inventory.view.App",
            "type": "XML",
            "async": true,
		    "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.93",
            "libs": {
                "sap.ui.core": {},
		        "sap.m": {},
                "sap.ui.layout":{}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "resources": {
            "css": [
              {
                "uri": "css/style.css"
              }
            ]
          }
    }
}